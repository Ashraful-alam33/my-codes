package stringash;

public class StringASH 
{

    public static void main(String[] args) 
    {
        findArea(5.4);       
    }
    public static void findArea(double r)
    {
      double pi = 3.1416;
      double area = pi *r*r;
      System.out.println(area);
              
         
    
    }
    
    
}
